{
  "type": "module",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --es-module-specifier-resolution=node dist/index.js\"",
    "build": "tsc",
    "format": "prettier \"**/*.ts\" --write",
    "typegen": "pgtyped -c pgTypedConfig.json"
  },
  "dependencies": {
    "@pgtyped/cli": "0.4.5",
    "@pgtyped/query": "0.4.5",
    "dotenv": "8.2.0",
    "pg": "7.18.2"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/node": "13.9.5",
    "@types/pg": "7.14.3",
    "concurrently": "5.1.0",
    "nodemon": "2.0.2",
    "prettier": "2.0.2",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": "13.12.0"
  },
  "license": "MIT"
}
