{
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node dist/index.js\"",
    "build": "tsc",
    "format": "prettier \"**/*.ts\" --write",
    "typegen": "pgtyped -c pgTypedConfig.json",
    "deploy": "git push heroku master"
  },
  "dependencies": {
    "@pgtyped/cli": "0.4.5",
    "@pgtyped/query": "0.4.5",
    "dotenv": "8.2.0",
    "moment": "2.29.1",
    "node-fetch": "2.6.1",
    "pg": "8.4.1",
    "pixl-xml": "1.0.13"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/node": "14.14.0",
    "@types/node-fetch": "2.5.7",
    "@types/pg": "7.14.5",
    "@types/pixl-xml": "1.0.1",
    "concurrently": "5.3.0",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": "15.0.1"
  },
  "license": "MIT"
}
